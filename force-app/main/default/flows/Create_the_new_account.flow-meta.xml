    <decisions>
        <name>checked_the_status</name>
        <label>checked the status</label>
        <locationX>1238</locationX>
        <locationY>242</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>is_status_is_draft</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>get_order_record_for_status.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Draft</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Account_Form</targetReference>
            </connector>
            <label>is status is draft</label>
        </rules>
        <rules>
            <name>other_than_draft</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>get_order_record_for_status.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Activated</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>get_order_record_for_status.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Processing</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>get_order_record_for_status.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Payment Pending</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>get_order_record_for_status.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Allotment in Process</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>tesy</targetReference>
            </connector>
            <label>other than draft</label>
        </rules>
        <rules>
            <name>Vehicle_owner_should_not_changed_after_this_stage</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>get_order_record_for_status.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RTO Registration</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>get_order_record_for_status.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Ready For Delivery</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>get_order_record_for_status.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Vehicle Delivered</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>get_order_record_for_status.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Order Cancelled</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Should_not_changed_the_vehicle_owner</targetReference>
            </connector>
            <label>Vehicle owner should not changed after this stage</label>
        </rules>
    </decisions>
